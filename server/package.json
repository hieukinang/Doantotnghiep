{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "MERN_SHOP",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "server": "nodemon server.js",
    "server:prod": "SET NODE_ENV=production& nodemon server.js",
    "client": "npm start --prefix client",
